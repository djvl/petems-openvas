{
  "name": "petems-openvas",
  "version": "0.1.0",
  "author": "Peter Souter",
  "summary": "Module for installing and managing OpenVas",
  "license": "Apache-2.0",
  "source": "git://github.com/petems/petems-openvas",
  "project_page": "https://github.com/petems/petems-openvas",
